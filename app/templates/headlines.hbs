<div id="rightNavContainer">
  {{#each model as |headline|}}
    <div id={{headline.scrollId}} {{action "scrollToHeadline" headline.classId}} class="right-nav-icon"></div>
  {{/each}}
</div>

<div id="headlinesContainer">
{{#each model as |headline|}}
  {{show-headline showHeadline='showHeadline' toggleDetail='toggleDetail' scrollToHeadline='scrollToHeadline' getMountain='getMountain' getChart='getChart' headline=headline}}
{{/each}}
</div>
